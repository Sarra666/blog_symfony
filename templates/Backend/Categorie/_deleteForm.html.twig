<form action="{{ path('admin.categorie.delete', {id: categorie.id}) }}" method="POST" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cette catégorie ?')">
	<input type="hidden" name="token" value="{{ csrf_token('delete' ~ categorie.id) }}">
	<button type="submit" class="btn btn-danger">Supprimer</button>
</form>
